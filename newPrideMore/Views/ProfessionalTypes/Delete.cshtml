@model newPrideMore.Models.ProfessionalType

@{
    ViewData["Title"] = "Excluir: Profissão | Especialidade";
}

<h2>Excluir</h2>

<h4>@Html.DisplayFor(model => model.Speciality)</h4>
<hr />
<h4>Você tem certeza que quer excluir?</h4>
<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Sim" class="btn btn-danger" />
</form>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-3">
            <span>Profissão | Especialidade</span>
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.Speciality)
        </dd>
    </dl>

    
    <div>
        <a asp-action="Index">← Voltar</a>
    </div>
</div>
