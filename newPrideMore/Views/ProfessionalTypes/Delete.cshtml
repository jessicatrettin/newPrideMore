@model newPrideMore.Models.ProfessionalType

@{
    ViewData["Title"] = "Excluir: Profissão | Especialidade";
}

<h2>Excluir</h2>

<h4>Profissão | Especialidade</h4>
<hr />
<h4>Você tem certeza que quer excluir?</h4>
<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Sim" class="btn btn-danger" />
</form>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            <span>Profissão</span>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Profission)
        </dd>
        <dt class="col-sm-2">
            <span>Especialidade</span>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Speciality)
        </dd>
    </dl>

    
    <div>
        <a asp-action="Index">← Voltar</a>
    </div>
</div>
